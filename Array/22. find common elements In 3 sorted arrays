class Solution:
    def commonElements(self, arr1, arr2, arr3):
        
        intersec1 = set(arr1) & set(arr2)
        intersec2 = intersec1 & set(arr3)

        result = sorted(list(intersec2))
        
        if not result:
            return [-1]

        return result
    
ob = Solution()



class Solution:
    def commonElements(self, arr1, arr2, arr3):
        i=j=k=0
        result =[]
        prev =None

        while i<len(arr1) & j<len(arr2) & k<len(arr3):

            if arr1[i] == arr2[j] == arr3[k]:
                if prev != result:
                    result.append(arr1[i])
                i += 1
                j += 1
                k += 1

            elif arr1[i]<arr2[j] :
                i+= 1
            elif arr1[j] < arr3[k]:
                j += 1
            else:
                k += 1

            return result if result else[-1]
