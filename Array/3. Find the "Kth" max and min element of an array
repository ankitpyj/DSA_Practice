# arr = [2, 1, 3, 10, 5, 6]
# k = 1
# print("Kth smallest:", kthsmallest(arr, k))



def kthsmallest(arr,k):
    n = len(arr)

    for i in range(n):
        for j in range(0,n-i-1):
            if arr[j] > arr[j+1]:
                arr[j] , arr[j+1] = arr[j+1] , arr[j]
    print(arr)
    return arr[k]

print(kthsmallest([1,3,2,4,9,6,8,7],3)) 
