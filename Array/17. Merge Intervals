class Solution(object):
    def merge(self,arr):
        arr.sort()

        result = [arr[0]]



    # merge :- 

        for i in range(1,len(arr)):

            last = result[-1]
            cur = arr[i]

            last_start,last_end = last
            cur_start, cur_end = cur



            if cur_start <= last_end:
                last[1]=max(last_end,cur_end)

            else:
                result.append(cur)

        return result


ob=Solution()
        
