def find_min(arr):
    min_num = arr[0]
    for n in arr:
        if n < min_num:
            min_num = n
    return min_num


def find_max(arr):
    max_num = arr[0]
    for n in arr:
        if n > max_num:
            max_num = n
    return max_num

print(find_min([1,2,3,4,5]))   #Output 1
print(find_max([5,1,3,4,2]))   #Output 5
