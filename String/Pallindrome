class Solution:
    def palindrome(self,s):
        i, j = 0, len(s) - 1  
        while i < j:           
            if s[i] != s[j] :
                return False

            i += 1
            j -= 1
        return True
    
ob = Solution()

print(ob.palindrome("madam")) 
print(ob.palindrome("hello"))
