def subsequences_by_mask(s):
    n = len(s)
    result = []
    
    for mask in range(1 << n):  # from 0 to 2^n - 1
        subseq = ""
        for i in range(n):
            if mask & (1 << i):   # check if i-th bit is set
                subseq += s[i]
        result.append(subseq)
    
    return result

s = "abc"
print(subsequences_by_mask(s))
