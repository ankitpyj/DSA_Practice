class Solution:
    # Function to find values in array equal to their indices
    def valueEqualToIndex(self, arr):
        result=[]
        for i in range(len(arr)):
            if arr[i] == i+1:
                result.append(i+1)
        return result
    

ob = Solution()
print(ob.valueEqualToIndex([15, 2, 45, 4, 7]))
